{"ast":null,"code":"import axios from \"axios\";\nexport var getUser = function getUser() {\n  return function (dispatch) {\n    return new Promise(function (resolve, reject) {\n      var URLAPI = process.env.NEXT_PUBLIC_URL_API_WITH_SLASH;\n      axios.get(\"\".concat(URLAPI, \"profile\"), {\n        withCredentials: true\n      }).then(function (res) {\n        resolve(res);\n        dispatch({\n          type: \"GET_USER\",\n          payload: res.data.data\n        });\n      })[\"catch\"](function (err) {\n        reject(err);\n      });\n    });\n  };\n};\nexport var updateUser = function updateUser(data, id) {\n  return function (dispatch) {\n    return new Promise(function (resolve, reject) {\n      var URLAPI = process.env.NEXT_PUBLIC_URL_API_WITH_SLASH;\n      axios.patch(\"\".concat(URLAPI, \"user/\").concat(id), data, {\n        withCredentials: true\n      }).then(function (res) {\n        dispatch(getUser());\n        resolve(res);\n      })[\"catch\"](function (err) {\n        reject(err);\n      });\n    });\n  };\n};\nexport var checkPin = function checkPin(data, id) {\n  return function (dispatch) {\n    return new Promise(function (resolve, reject) {\n      var URLAPI = process.env.NEXT_PUBLIC_URL_API_WITH_SLASH;\n      axios.post(\"\".concat(URLAPI, \"loginPIN/\").concat(id), data, {\n        withCredentials: true\n      }).then(function (res) {\n        resolve(res);\n      })[\"catch\"](function (err) {\n        reject(new Error(err));\n      });\n    });\n  };\n};","map":null,"metadata":{},"sourceType":"module"}